    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-unicorn": "~29.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.3"
  },
