{
  "name": "@services/api-example",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "verify": "eslint --max-warnings=0 --ext .ts,.tsx,.js,.jsx ./",
    "verify:fix": "eslint --fix --max-warnings=0 --ext .ts,.tsx,.js,.jsx ./",
    "cdk": "cdk",
    "cdk:synth": "cdk synth",
    "cdklocal": "cdklocal",
    "generate:diagram": "npm-run-all cdk:synth generate:diagram:drawio generate:diagram:image",
    "generate:diagram:drawio": "cfn-dia draw.io --cdk-output cdk.out --output-file docs/cdk-diagram.drawio --ci-mode --skip-synth",
    "generate:diagram:image": "drawio docs/cdk-diagram.drawio -o docs/cdk-diagram.png"
  },
  "dependencies": {
    "aws-sdk": "^2.617.0",
    "source-map-support": "^0.5.9"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.97.0",
    "@aws-cdk/aws-apigateway": "^1.97.0",
    "@aws-cdk/aws-cloudfront-origins": "~1.97.0",
    "@aws-cdk/aws-iam": "^1.97.0",
    "@aws-cdk/aws-lambda": "^1.97.0",
    "@aws-cdk/aws-route53-targets": "~1.97.0",
    "@aws-cdk/aws-s3": "^1.97.0",
    "@aws-cdk/aws-s3-deployment": "~1.97.0",
    "@aws-cdk/core": "1.97.0",
    "@mhlabs/cfn-diagram": "^1.1.24",
    "@sls-next/cdk-construct": "1.19.0-alpha.36",
    "@sls-next/lambda-at-edge": "1.8.0-alpha.31",
    "@types/jest": "^26.0.22",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "~4.21.0",
    "@typescript-eslint/parser": "~4.21.0",
    "aws-cdk": "^1.97.0",
    "aws-cdk-local": "^1.65.4",
    "draw.io-export": "^0.1.1",
    "eslint": "~7.23.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-prettier": "~3.3.1",
    "eslint-plugin-react": "~7.23.1",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-unicorn": "~29.0.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "source-map-support": "^0.5.16",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  }
